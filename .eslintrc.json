{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jasmine": true
  },
  "extends": ["airbnb-base", "plugin:jasmine/recommended"],
  "plugins": ["jasmine"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "prefer-destructuring": 0,
    "no-unused-vars": 1
  }
}
